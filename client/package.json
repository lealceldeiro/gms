{
  "name": "client",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch=true",
    "build": "ng build --prod --deploy-url \"\" --base-href \"./\"",
    "doc:build": "compodoc -p src/tsconfig.app.json",
    "doc:serve": "compodoc -s",
    "doc:build:serve": "compodoc -p src/tsconfig.app.json -s --hideGenerator -n \"GMS Angular Client Documentation\"",
    "test": "ng test --watch=false",
    "test:watch": "ng test --watch=true",
    "test:watch:no-sourcemap": "ng test --watch=true --sourceMap=false",
    "test:coverage": "ng test --watch=false --code-coverage",
    "lint:ng": "ng lint",
    "lint:standard": "npx standardx **/*.ts **/*.js",
    "lint-parallel": "npm-run-all --aggregate-output --parallel lint:*",
    "e2e": "ng e2e",
    "test:e2e": "npm-run-all --aggregate-output --parallel test e2e",
    "test:e2e:coverage": "npm-run-all --aggregate-output --parallel e2e test:coverage",
    "postinstall": "npm run script:git:hooks",
    "script:git:hooks": "copyfiles -f -s ../scripts/git/hooks/* ../.git/hooks"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@ngx-pwa/local-storage": "^8.2.2",
    "@types/lodash": "^4.14.144",
    "@types/object-hash": "^1.3.0",
    "bootstrap": "^4.3.1",
    "copyfiles": "^2.1.1",
    "core-js": "^3.2.1",
    "lodash": "^4.17.15",
    "ngx-cookie": "^4.0.2",
    "ngx-toastr": "^10.2.0",
    "ngx-ui-loader": "^8.0.0",
    "object-hash": "^2.0.0",
    "rxjs": "^6.5.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.10",
    "@compodoc/ngd-cli": "^2.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "codelyzer": "^5.1.0",
    "coveralls": "^3.0.6",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "standardx": "^5.0.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "~3.5.3"
  },
  "standardx": {
    "globals": [
      "jasmine",
      "expect",
      "it",
      "describe",
      "beforeEach",
      "beforeAll",
      "afterEach",
      "afterAll",
      "spyOn",
      "HTMLElement",
      "ErrorEvent",
      "Event"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
